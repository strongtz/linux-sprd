	idle-states {
		entry-method = "arm,psci";
		CORE_PD: core_pd {
			compatible = "arm,idle-state";
			entry-latency-us = <1000>;
			exit-latency-us = <500>;
			min-residency-us = <2500>;
			local-timer-stop;
			arm,psci-suspend-param = <0x00010000>;
		};
	};

	cpufreq_clus0: cpufreq-clus0 {
			cpufreq-cluster-id = <0>;
			nvmem-cells = <&dvfs_bin_cpu0>;
			nvmem-cell-names = "dvfs_bin";
			sprd,cpufreq-temp-threshold = <65>;
			clocks = <&aon_clk CLK_CORE0>,
				<&aon_clk CLK_CORE1>,
				<&aon_clk CLK_CORE2>,
				<&aon_clk CLK_CORE3>,
				<&pll CLK_TWPLL_768M>,
				<&mpll CLK_MPLL0>;
			clock-names = "core_clk",
					"core1_clk",
					"core2_clk",
					"core3_clk",
					"low_freq_clk_parent",
					"high_freq_clk_parent";
			clock-latency = <50000>;
			voltage-tolerance = <6413>;
			volt-share-masters-bits = <0x1>;
			operating-points = <
				/* kHz	uV */
				1200000	1121875
				1100000	1075000
				1000000	1028125
				884000	975000
				768000	918750>;
